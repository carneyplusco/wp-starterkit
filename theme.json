{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "layout": {
      "contentSize": "var(--bs-4-wp-content-max)",
      "wideSize": "var(--bs-4-wp-block-max)"
    },
    "border": {
      "color": false,
      "radius": false,
      "style": false,
      "width": false
    },
    "spacing": {
      "spacingSizes": [
        {
          "size": "var(--bs-4-wp-spacing-1)",
          "slug": "1",
          "name": "1"
        },
        {
          "size": "var(--bs-4-wp-spacing-2)",
          "slug": "2",
          "name": "2"
        },
        {
          "size": "var(--bs-4-wp-spacing-3)",
          "slug": "3",
          "name": "3"
        },
        {
          "size": "var(--bs-4-wp-spacing-4)",
          "slug": "4",
          "name": "4"
        },
        {
          "size": "var(--bs-4-wp-spacing-5)",
          "slug": "5",
          "name": "5"
        },
        {
          "size": "var(--bs-4-wp-spacing-6)",
          "slug": "6",
          "name": "6"
        },
        {
          "size": "var(--bs-4-wp-spacing-sm)",
          "slug": "7",
          "name": "7"
        },
        {
          "size": "var(--bs-4-wp-spacing-md)",
          "slug": "8",
          "name": "8"
        },
        {
          "size": "var(--bs-4-wp-spacing-lg)",
          "slug": "9",
          "name": "9"
        },
        {
          "size": "var(--bs-4-wp-spacing-xl)",
          "slug": "10",
          "name": "10"
        }
      ],
      "blockGap": false,
      "units": ["%", "ch", "vh", "vw", "em", "rem"]
    },
    "typography": {
      "dropCap": false,
      "fontFamilies": [
        {
          "name": "Sans Serif",
          "slug": "serif",
          "fontFamily": "\"Georgia\", serif"
        },
        {
          "name": "Sans",
          "slug": "sans",
          "fontFamily": "\"Helvetica Neue\", Arial, sans-serif"
        }
      ]
    },
    "color": {
      "defaultPalette": false,
      "defaultGradients": false
    },
    "blocks": {
      "core/heading": {
        "spacing": {
          "blockGap": false,
          "padding": false,
          "margin": false
        },
        "typography": {
          "customFontSize": false,
          "fontSizes": []
        }
      },
      "core/paragraph": {
        "spacing": {
          "blockGap": false,
          "padding": false,
          "margin": false
        },
        "typography": {
          "customFontSize": false,
          "fontSizes": []
        }
      },
      "core/group": {
        "spacing": {
          "blockGap": true,
          "padding": true,
          "margin": true
        }
      },
      "core/media-text": {
        "spacing": {
          "blockGap": true,
          "padding": true,
          "margin": true
        }
      },
      "core/columns": {
        "typography": {
          "customFontSize": false,
          "fontSizes": []
        },
        "spacing": {
          "blockGap": true,
          "padding": true,
          "margin": true
        }
      },
      "core/column": {
        "typography": {
          "customFontSize": false,
          "fontSizes": []
        },
        "spacing": {
          "blockGap": true,
          "padding": true,
          "margin": true
        }
      },
      "core/buttons": {
        "spacing": {
          "blockGap": true,
          "padding": true,
          "margin": true
        }
      },
      "core/button": {
        "typography": {
          "customFontSize": false,
          "fontSizes": [],
          "fontStyle": false,
          "fontWeight": false,
          "lineHeight": false,
          "textTransform": false,
          "letterSpacing": false,
          "textDecoration": false
        },
        "border": {
          "radius": false
        },
        "color": {
          "text": false,
          "custom": false
        },
        "spacing": {
          "padding": false,
          "margin": false
        }
      }
    }
  },
  "styles": {
    "spacing": {
      "blockGap": "var(--wp--preset--spacing--4)",
      "padding": {
        "top": "0",
        "right": "var(--wp--preset--spacing--4)",
        "bottom": "0",
        "left": "var(--wp--preset--spacing--4)"
      }
    },
    "blocks": {
      "core/group": {
        "spacing": {
          "padding": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          },
          "margin": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          }
        }
      },
      "core/row": {
        "spacing": {
          "padding": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          },
          "margin": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          }
        }
      },
      "core/stack": {
        "spacing": {
          "padding": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          },
          "margin": {
            "top": "0",
            "right": "0",
            "bottom": "0",
            "left": "0"
          }
        }
      }
    }
  }
}
