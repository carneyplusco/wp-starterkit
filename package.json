{
  "name": "test-theme",
  "version": "1.0.0",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@parcel/transformer-sass": "^2.8.2",
    "@prettier/plugin-php": "^0.17.6",
    "bootstrap": "^5.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "parcel": "^2.8.2",
    "prettier": "^2.5.1",
    "prettier-eslint": "^11.0.0",
    "sharp": "^0.31.3"
  },
  "scripts": {
    "test": "rm -rf dist/ && npx parcel watch 'templates/blocks/**/*.js' 'templates/blocks/**/*.scss' 'assets/scripts/*.js' 'assets/styles/main.scss' 'assets/styles/editor.scss' 'assets/images/*' --no-source-maps",
    "start": "rm -rf dist/ && npx parcel watch 'assets/scripts/*.js' 'assets/styles/main.scss' 'assets/styles/editor.scss' 'assets/images/*' --no-source-maps",
    "build": "rm -rf dist/ && rm -rf .parcel-cache && npx parcel build 'assets/scripts/*.js' 'assets/styles/main.scss' 'assets/styles/editor.scss' 'assets/styles/pardot.scss' 'assets/images/*' --no-source-maps",
    "deploy:staging": "rsync -azvh --progress --exclude '.git' --exclude 'node_modules' --exclude 'assets' --exclude '.parcel-cache' . test-server:/home/master/applications/test-theme/public_html/wp-content/themes/test-theme --delete-after"
  },
  "browserslist": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ],
  "author": "Carney",
  "license": "ISC",
  "dependencies": {
    "@wordpress/blocks": "^12.2.0"
  }
}
